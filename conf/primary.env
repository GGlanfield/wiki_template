APACHE_HOSTNAME=localhost # should equal the ip/domain name of your server
EMAIL_CONTACT=someuser@example.com
WIKI_PATH=/wiki
WIKI_NAME=MyWiki
WIKI_DB_SERVICE_NAME=wiki_db # this should be the same as the wiki db service name in docker-compose.yml
WIKI_ADMIN_USER_PASSWORD=somecomplexadminpassword
WIKI_SECRET_KEY=1234 # run "openssl rand -hex 32" to generate a good one

MYSQL_DATABASE=wiki_db # the name of the database
MYSQL_USER=wikiuser # the wiki db username
MYSQL_PASSWORD=change_to_some_long_complex_password # the wiki db password



CERTIFICATE_PATH=/etc/ssl_certificates # internal env - do not edit
